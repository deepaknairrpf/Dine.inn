<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.grey_hat.dineinn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dineinn"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppRedefTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.grey_hat.signedIn"
            android:label="@string/title_activity_signed_in"

            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.grey_hat.dineinn.MainActivity" />
        </activity>
    </application>

</manifest>